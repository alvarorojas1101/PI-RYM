{
  "version": 2,
  "builds": [
    {
      "src": "back/src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "front/index.html"
    }
  ]
}
